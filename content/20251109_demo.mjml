    <mj-section
      background-color="#ffffff"
      background-repeat="repeat"
      padding-bottom="24px"
      padding-left="24px"
      padding-right="24px"
      padding-top="24px"
      padding="24px 24px 24px 24px"
      text-align="center"
      vertical-align="top"
    >
      <mj-column>
        <mj-text
          font-family="Helvetica, Arial, sans-serif"
          padding-bottom="0px"
          padding-left="10px"
          padding-right="10px"
          padding-top="0px"
          padding="0px 10px 0px 10px"
        >
          <p
            class="text-build-content"
            style="margin: 10px 0; margin-bottom: 10px"
          >
            <span
              style="
                color: #595e61;
                font-family: Arial;
                font-size: 16px;
                line-height: 23px;
              "
              >With love and in solidarity,</span
            ><br /><span
              style="
                color: #595e61;
                font-family: Arial;
                font-size: 16px;
                line-height: 23px;
              "
              >Bed-Stuy Strong organizers</span
            >
          </p>
        </mj-text>
      </mj-column>
    </mj-section>
    <mj-section
      background-color="#538f0b"
      background-repeat="repeat"
      padding-bottom="10px"
      padding-left="30px"
      padding-right="30px"
      padding-top="10px"
      padding="10px 30px 10px 30px"
      text-align="center"
      vertical-align="middle"
    >
      <mj-group vertical-align="middle">
        <mj-column vertical-align="middle" width="33%">
          <mj-text
            font-family="Helvetica, Arial, sans-serif"
            padding-bottom="0px"
            padding-left="10px"
            padding-right="10px"
            padding-top="0px"
            padding="0px 10px 0px 10px"
          >
            <p
              style="
                text-align: left;
                margin: 10px 0;
                margin-top: 10px;
                margin-bottom: 10px;
              "
            >
              <span
                style="
                  line-height: 22px;
                  letter-spacing: normal;
                  font-size: 16px;
                  color: #ffffff;
                  text-align: left;
                  font-family: Arial;
                "
                ><b>Keep in touch</b></span
              >
            </p>
          </mj-text>
        </mj-column>
        <mj-column vertical-align="middle" width="67%">
          <mj-include path="../includes/social.mjml">
        </mj-column>
      </mj-group>
    </mj-section>
    <mj-section
      background-color="transparent"
      background-repeat="repeat"
      padding-bottom="30px"
      padding-left="20px"
      padding-right="20px"
      padding-top="10px"
      padding="10px 20px 30px 20px"
      text-align="center"
    >
      <mj-column>
        <mj-text
          font-family="Helvetica, Arial, sans-serif"
          padding-bottom="0px"
          padding-left="0px"
          padding-right="0px"
          padding-top="0px"
          padding="0px 0px 0px 0px"
        >
          <p
            style="
              text-align: center;
              margin: 10px 0;
              margin-top: 10px;
              margin-bottom: 10px;
            "
          >
            <span
              style="
                text-align: center;
                line-height: 23px;
                letter-spacing: normal;
                font-size: 16px;
                color: #000000;
                text-align: left;
                font-family: Arial;
              "
              ><br /></span
            ><span
              style="
                text-align: center;
                line-height: 23px;
                letter-spacing: normal;
                font-size: 16px;
                color: #515557;
                text-align: left;
                font-family: Arial;
              "
              >This e-mail has been sent to </span
            ><span
              style="
                text-align: center;
                line-height: 23px;
                letter-spacing: normal;
                font-size: 16px;
                color: #515557;
                text-align: left;
                font-family: Arial;
              "
              ><b>[[EMAIL_TO]]</b></span
            ><span
              style="
                text-align: center;
                line-height: 23px;
                letter-spacing: normal;
                font-size: 16px;
                color: #515557;
                text-align: left;
                font-family: Arial;
              "
              >, click </span
            ><a href="[[UNSUB_LINK_EN]]" target="_blank"
              ><span
                ><b
                  ><u
                    ><span
                      style="
                        text-align: center;
                        line-height: 23px;
                        letter-spacing: normal;
                        font-size: 16px;
                        color: #515557;
                        text-align: left;
                        font-family: Arial;
                      "
                      ><b><u>here</u></b></span
                    ></u
                  ></b
                ></span
              ></a
            ><span
              style="
                text-align: center;
                line-height: 23px;
                letter-spacing: normal;
                font-size: 16px;
                color: #515557;
                text-align: left;
                font-family: Arial;
              "
            >
              to unsubscribe.</span
            >
          </p>
        </mj-text>
      </mj-column>
    </mj-section>
  </mj-body>
